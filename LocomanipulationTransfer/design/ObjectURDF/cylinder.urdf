<?xml version="1.0" ?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="cylinder">

    <link name="cylinder">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
            <mass value="2.5"/>
            <inertia ixx="0.163" ixy="0.0" ixz="0.0" iyy="0.221" iyz="0.0" izz="0.163"/>
        </inertial>

        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="./meshes/cylinder/cylinder.obj" scale="1 1 1"/>
            </geometry>
        </visual>

        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="./meshes/cylinder_collision/cylinder_collision.obj" scale="1 1 1"/>
            </geometry>
        </collision>
    </link>
</robot>